<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">delhivery_lastmile_awb_form.awb_form_data_source</item>
        </item>
        <item name="label" xsi:type="string" translate="true">Manage AWB Information</item>
        <item name="template" xsi:type="string">templates/form/collapsible</item>
    </argument>
    <settings>
        <buttons>
            <button name="back" class="Delhivery\Lastmile\Block\Adminhtml\Awb\Edit\Buttons\Back"/>
            <button name="delete" class="Delhivery\Lastmile\Block\Adminhtml\Awb\Edit\Buttons\Delete"/>
            <button name="reset" class="Delhivery\Lastmile\Block\Adminhtml\Awb\Edit\Buttons\Reset"/>
            <button name="save" class="Delhivery\Lastmile\Block\Adminhtml\Awb\Edit\Buttons\Save"/>
            <button name="save_and_continue" class="Delhivery\Lastmile\Block\Adminhtml\Awb\Edit\Buttons\SaveAndContinue"/>
        </buttons>
        <namespace>delhivery_lastmile_awb_form</namespace>
        <dataScope>data</dataScope>
        <deps>
            <dep>delhivery_lastmile_awb_form.awb_form_data_source</dep>
        </deps>
    </settings>
    <dataSource name="awb_form_data_source">
        <argument name="data" xsi:type="array">
            <item name="js_config" xsi:type="array">
                <item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
            </item>
        </argument>
        <settings>
            <submitUrl path="delhivery_lastmile/awb/save"/>
        </settings>
        <dataProvider class="Delhivery\Lastmile\Model\Awb\DataProvider" name="awb_form_data_source">
            <settings>
                <requestFieldName>awb_id</requestFieldName>
                <primaryFieldName>awb_id</primaryFieldName>
            </settings>
        </dataProvider>
    </dataSource>
    <fieldset name="general">
        <settings>
            <label translate="true">Manage AWB Information</label>
        </settings>
        <field name="awb_id" formElement="input">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <visible>false</visible>
                <dataScope>awb_id</dataScope>
            </settings>
        </field>
        <field name="awb" sortOrder="10" formElement="input">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
                <dataType>text</dataType>
                <label translate="true">AWB</label>
                <dataScope>awb</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
            </settings>
        </field>
        <field name="shipment_id" formElement="input" sortOrder="20">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Shipment Id</label>
                <dataScope>shipment_id</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
                <notice>shipment_id</notice>
            </settings>
        </field>
        <field name="shipment_to" formElement="input" sortOrder="30">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Shipment To</label>
                <dataScope>shipment_to</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
            </settings>
        </field>
        <field name="state" formElement="select" sortOrder="40">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">State</label>
                <dataScope>state</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
            </settings>
            <formElements>
                <select>
                    <settings>
                        <options class="Delhivery\Lastmile\Model\Awb\Source\State"/>
                    </settings>
                </select>
            </formElements>
        </field>
        <field name="status" formElement="select" sortOrder="50">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Status</label>
                <dataScope>status</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
                <notice>status</notice>
            </settings>
            <formElements>
                <select>
                    <settings>
                        <options class="Delhivery\Lastmile\Model\Awb\Source\Status"/>
                    </settings>
                </select>
            </formElements>
        </field>
        <field name="status_type" formElement="select" sortOrder="60">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Status Type</label>
                <dataScope>status_type</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
            </settings>
            <formElements>
                <select>
                    <settings>
                        <options class="Delhivery\Lastmile\Model\Awb\Source\StatusType"/>
                    </settings>
                </select>
            </formElements>
        </field>
        <field name="pickup_location_id" formElement="input" sortOrder="70">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Pickup Location Id</label>
                <dataScope>pickup_location_id</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
            </settings>
        </field>
        <field name="return_address" formElement="textarea" sortOrder="80">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Return Address</label>
                <dataScope>return_address</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
            </settings>
        </field>
        <field name="shipment_length" formElement="input" sortOrder="90">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Shipment Length</label>
                <dataScope>shipment_length</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
            </settings>
        </field>
        <field name="shipment_width" formElement="input" sortOrder="100">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Shipment Width</label>
                <dataScope>shipment_width</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
            </settings>
        </field>
        <field name="shipment_height" formElement="input" sortOrder="110">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Shipment Height</label>
                <dataScope>shipment_height</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
            </settings>
        </field>
        <field name="status_date_time" formElement="input" sortOrder="120">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Status Date Time</label>
                <dataScope>status_date_time</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
            </settings>
        </field>
        <field name="upl" formElement="input" sortOrder="130">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Upl</label>
                <dataScope>upl</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
            </settings>
        </field>
        <field name="orderid" formElement="input" sortOrder="140">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">awb</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Order Id</label>
                <dataScope>orderid</dataScope>
                <validation>
                    <rule name="required-entry" xsi:type="boolean">true</rule>
                </validation>
            </settings>
        </field>
    </fieldset>
</form>
