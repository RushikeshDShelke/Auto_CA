        <plugin name="MobileLoginAccountLoginPost" type="Meetanshi\Mobilelogin\Plugin\Customer\Loginpost" sortOrder="10"/>
    </type>
    <type name="Magento\Customer\Controller\Account\EditPost">
        <plugin name="MobileLoginAccountEditPost" type="Meetanshi\Mobilelogin\Plugin\Customer\EditPost" sortOrder="10"/>
    </type>

    <preference for="Meetanshi\Mobilelogin\Api\PostManagementInterface" type="Meetanshi\Mobilelogin\Model\Api\PostManagement"/>
    <preference for="Meetanshi\Mobilelogin\Api\ResetPasswordInterface" type="Meetanshi\Mobilelogin\Model\Api\ResetPassword"/>
    <preference for="Meetanshi\Mobilelogin\Api\OtpVerifyInterface" type="Meetanshi\Mobilelogin\Model\Api\OtpVerify"/>

    <preference for="Meetanshi\Mobilelogin\Api\AccountLoginInterface" type="Meetanshi\Mobilelogin\Model\Api\AccountLogin"/>
    <preference for="Meetanshi\Mobilelogin\Api\AccountCreateInterface" type="Meetanshi\Mobilelogin\Model\Api\AccountCreate"/>

