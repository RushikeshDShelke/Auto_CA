<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <delhivery>				
				<active>1</active>
                <!--[Carrier's title default value]-->
                <title>Delhivery Lastmile</title>
                <!--[Carrier's model class]-->
                <model>Delhivery\Lastmile\Model\Carrier\Shipping</model>
                <!--[Error message displays on storefront if carrier's methods unavailable]-->
                <specificerrmsg>This shipping method is currently unavailable. If you would like to ship using this shipping method, please contact us.</specificerrmsg>
            </delhivery>
        </carriers>
    </default>
</config>
