<?xml version="1.0" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">

                <resource id="Kellton_Custominvoice::parent" title="Custominvoice" sortOrder="200" >
                    <resource id="Kellton_Custominvoice::customcinvoice" title="Manage Customer Invoice" sortOrder="0" />
                    <resource id="Kellton_Custominvoice::customcminvoice" title="Manage Supplier Invoice" sortOrder="2" />
                </resource>


                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Kellton_Custominvoice::config_kellton_custominvoice" title="invoice"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>